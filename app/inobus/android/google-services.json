{
  "project_info": {
    "project_number": "${env.GOOGLE_SERVICES_PROJECT_NUMBER}",
    "project_id": "inobusapp",
    "storage_bucket": "inobusapp.appspot.com"
  },
  "client": [
    {
      "client_info": {
        "mobilesdk_app_id": "${env.GOOGLE_SERVICES_MOBILESDK_APP_ID}",
        "android_client_info": {
          "package_name": "com.example.inobus"
        }
      },
      "oauth_client": [
        {
          "client_id": "${env.GOOGLE_SERVICES_CLIENT_ID}",
          "client_type": 1,
          "android_info": {
            "package_name": "com.example.inobus",
            "certificate_hash": "${env.GOOGLE_SERVICES_CERTIFICATE_HASH}"
          }
        },
        {
          "client_id": "${env.GOOGLE_SERVICES_CLIENT_ID2}",
          "client_type": 3
        }
      ],
      "api_key": [
        {
          "current_key": "${env.GOOGLE_SERVICES_CURRENT_KEY}"
        }
      ],
      "services": {
        "appinvite_service": {
          "other_platform_oauth_client": [
            {
              "client_id": "${env.GOOGLE_SERVICES_CLIENT_ID2}",
              "client_type": 3
            },
            {
              "client_id": "${env.GOOGLE_SERVICES_CLIENT_ID3}",
              "client_type": 2,
              "ios_info": {
                "bundle_id": "com.inobus.InobusApp"
              }
            }
          ]
        }
      }
    }
  ],
  "configuration_version": "1"
}